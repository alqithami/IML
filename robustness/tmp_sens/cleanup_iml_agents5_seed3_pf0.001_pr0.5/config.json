{
  "run": {
    "runs_dir": "runs"
  },
  "train": {
    "seed": 3
  },
  "env": {
    "name": "cleanup",
    "num_agents": 5,
    "kwargs": {}
  },
  "iml": {
    "enabled": true,
    "p_detect_true": 0.9,
    "p_detect_false": 0.01,
    "sanction": 0.5,
    "p_review": 0.2,
    "write_ledger": true,
    "rules": [
      "no_punishment_beam"
    ]
  },
  "ppo": {
    "total_steps": 2000000,
    "rollout_steps": 256,
    "gamma": 0.99,
    "gae_lambda": 0.95,
    "lr": 0.00025,
    "num_epochs": 4,
    "minibatch_size": 512,
    "clip_coef": 0.2,
    "ent_coef": 0.01,
    "vf_coef": 0.5,
    "max_grad_norm": 0.5,
    "device": "auto"
  }
}